{
  "name": "gatsby-plugin-linaria",
  "version": "2.0.0",
  "description": "Gatsby plugin for styling with Linaria",
  "main": "index.js",
  "homepage": "https://github.com/silvenon/gatsby-plugin-linaria#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/silvenon/gatsby-plugin-linaria.git"
  },
  "author": {
    "name": "Matija MarohniÄ‡",
    "email": "matija.marohnic@gmail.com",
    "url": "https://silvenon.com"
  },
  "contributors": [
    {
      "name": "Hyeseong Kim",
      "url": "https://cometkim.kr"
    }
  ],
  "bugs": {
    "url": "https://github.com/silvenon/gatsby-plugin-linaria/issues",
    "email": "matija.marohnic@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "linaria",
    "css",
    "css-in-js"
  ],
  "engines": {
    "node": ">=4.9.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir .",
    "prelint": "eslint --print-config ./index.js | eslint-config-prettier-check",
    "lint": "eslint .",
    "prepublishOnly": "yarn lint && yarn build"
  },
  "peerDependencies": {
    "gatsby": ">=2",
    "linaria": ">=1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "babel-eslint": "^10.0.3",
    "babel-preset-gatsby-package": "^0.4.2",
    "cross-env": "^7.0.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
