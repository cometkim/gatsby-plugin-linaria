{
  "name": "gatsby-plugin-linaria",
  "version": "1.0.1",
  "description": "Gatsby plugin for styling with Linaria",
  "main": "index.js",
  "repository": "https://github.com/silvenon/gatsby-plugin-linaria",
  "author": "Matija MarohniÄ‡ <matija.marohnic@gmail.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "linaria"
  ],
  "engines": {
    "node": ">=4.9.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run eslint-check && eslint ."
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "peerDependencies": {
    "gatsby": ">=2",
    "linaria": ">=1.0.0-beta"
  },
  "devDependencies": {
    "eslint": "^5.11.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3"
  }
}
