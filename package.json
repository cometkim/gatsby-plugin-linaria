{
  "name": "gatsby-plugin-linaria",
  "version": "2.0.0",
  "description": "Gatsby plugin for styling with Linaria",
  "main": "index.js",
  "repository": "https://github.com/silvenon/gatsby-plugin-linaria",
  "author": "Matija MarohniÄ‡ <matija.marohnic@gmail.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "linaria"
  ],
  "engines": {
    "node": ">=4.9.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir .",
    "prelint": "eslint --print-config ./index.js | eslint-config-prettier-check",
    "lint": "eslint .",
    "prepublishOnly": "yarn lint && yarn build"
  },
  "peerDependencies": {
    "gatsby": ">=2",
    "linaria": ">=1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "babel-eslint": "^10.0.3",
    "babel-preset-gatsby-package": "^0.2.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^3.0.0",
    "lint-staged": "^9.0.1",
    "prettier": "^1.18.2"
  },
  "dependencies": {}
}
